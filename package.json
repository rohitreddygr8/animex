{
  "name": "animex",
  "private": true,
  "version": "0.0.0",
  "description": "A free anime streaming web application made with React, TypeScript, SASS, Express.js, and GraphQL, using a gogoanime web-scraper.",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"cd ./client && vite\" \"cd ./server/src && cross-env NODE_ENV=development nodemon server.ts\"",
    "build": "concurrently \"cd ./client && tsc && vite build\" \"cd ./server && rimraf ./dist && tsc\"",
    "start": "cd ./server/dist && cross-env NODE_ENV=production node server.js",
    "preview": "cd ./client && vite preview",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rohitman47/animex.git"
  },
  "keywords": [
    "anime",
    "streaming",
    "react",
    "typescript",
    "sass",
    "graphql",
    "express"
  ],
  "bugs": {
    "url": "https://github.com/rohitman47/animex/issues"
  },
  "homepage": "https://github.com/rohitman47/animex#readme",
  "author": "rohitman47",
  "license": "MIT",
  "dependencies": {
    "@tanstack/react-query": "^4.0.10",
    "axios": "^0.27.2",
    "binascii": "^0.0.2",
    "cheerio": "^1.0.0-rc.10",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "puppeteer": "^15.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hls-player": "^3.0.7",
    "react-router-dom": "^6.3.0",
    "user-agents": "^1.0.1039"
  },
  "devDependencies": {
    "@testing-library/react": "^13.4.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.0.0",
    "@types/nodemailer": "^6.4.6",
    "@types/pg": "^8.6.5",
    "@types/postcss-preset-env": "^7.7.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.0",
    "concurrently": "^7.2.2",
    "jest": "^29.2.1",
    "nodemon": "^2.0.18",
    "postcss": "^8.4.14",
    "postcss-preset-env": "^7.7.2",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vite-plugin-svgr": "^2.2.0"
  }
}
